<?xml version="1.0" encoding="utf-8" standalone="yes" ?> 
<!--配置数据根据各个项目进行更新，初步编写，具体需要根据具体的线路确定-->
<MMI>
	<!-- 配置文件版本号 -->
	<Version>0.01</Version>
	<Project>产品合库</Project>
	
	<!--通信配置数据-->
	<Commu>
		<!-- 通信类析，0：串口，1：网络-->
		<type>0</type>
		<!--协议配置数据，0:安全通信协议，1：UDP协议-->
		<Protocol>0</Protocol>
		<!--红网配置-->
		<RedNet>/dev/ttyS0</RedNet>
		<!--蓝网配置-->
		<BlueNet>/dev/ttyS1</BlueNet>
	</Commu>
	
	<!--MMI屏幕配置-->
	<Screen>
		<!--MMI类型，1：10英寸，2：12英寸  -->
		<!--乌鲁木齐：1;越南:1;其他：2-->
		<type>2</type>
		<!--MMI宽度，10英寸：629，12英寸：1024  -->
		<!--乌鲁木齐:629;越南:629;其他:1024-->
		<width>1024</width>
		<!--MMI高度，10英寸：468，12英寸：768  -->
		<!--乌鲁木齐:468;越南:468;其他:768-->
		<height>768</height>
	</Screen>

	<!-- 区域配置数据 10英寸：11，12英寸：18  -->
	<!--乌鲁木齐，越南:T1区FontSize为11;其他：18-->
	<Area AreaName="A2" FontName = "vera_sans" FontSize="16" Color="128" BKColor="256" Weight="75"/>
	<Area AreaName="T1" FontName = "vera_sans" FontSize="18" Color="128" BKColor="256" Weight="63"/>
	
	<!--显示选项-->
	<Display>
		<!--ITC_CM or BLOC-CM-->
		<CMModeText>ITC_CM</CMModeText>
		<!--ITC_AM or BLOC-AM-->
		<AMModeText>ITC_AM</AMModeText>
		<!--冗余开关-->
		<RundantSwitch>1</RundantSwitch>
		<!--点式图片，BLOC，ITC -->
		<pic>ITC</pic>
		<!--终点站Ga cuối，下一站Ga tiếp显示文本配置-->
		<DestStaion>终点站:</DestStaion>
		<NextStation>下一站:</NextStation>
		<!--设置界面，1：无声音调节和亮度调节功能的界面， 2：有声音调节和亮度调节功能的界面-->
		<SettingsInterface>2</SettingsInterface>
		<!--设置界面，1：无雨雪模式功能的界面， 2：有雨雪模式功能的界面-->
		<SettingsRainOrSnow>2</SettingsRainOrSnow>
		<!--故障显示配置数据 1 显示，0 不显示40，80，100，200错误码对应错误信息-->
		<ErrInfo>1</ErrInfo>
		<!--点灯测试配置项 1 显示，0 不显示-->
		<ARLightShow>1</ARLightShow>
                <!--页面显示语言选择 0：中文  1 越南文-->
                <SelectLanguage>0</SelectLanguage>
                <!--界面显示风格：传统界面 1 ；重庆环线：0-->
                <SelectScreenStyle>1</SelectScreenStyle>
                <!--页面紧急制动速度后半圆弧显示 0：不显示  1：显示为红圈 2：显示为白圈-->
		<!--产品：1;贵阳:0;青岛:1;乌鲁木齐:0;越南:0;重庆环线:1;-->
                <SelectEbCircle>0</SelectEbCircle>
                <!--车次号显示风格（0代表车次号前三位为服务号后两位为序列号，1代表传统显示方式）：0 贵阳；1 其他线路-->
                <TrainNumStyle>0</TrainNumStyle>
                <!--是否具备FAO模式（1表示具备，0表示不具备）：1 青岛；0 其他线路-->
                <IsFaoStyle>1</IsFaoStyle>
                <!--选择速度指针风格 0：粗中细明显过度型，1：三角形-->
		<!--产品:1;贵阳:0;青岛:1;乌鲁木齐:0;越南:0;重庆环线:1;-->
                <SelectPointerStyle>0</SelectPointerStyle>
                <!--目标距离文本显示 0:不显示，1：显示-->
		<!--产品:1;贵阳:0;青岛:1;乌鲁木齐:0;越南:0;重庆环线:1;-->
                <TargetDistance>0</TargetDistance>
                <!--速度表盘显示  “110”：0，“120”：1，“140”：2-->
		<!--产品:1;贵阳:0;青岛:2;乌鲁木齐:0;越南:0;重庆环线:0;-->
                <SpeedStyle>0</SpeedStyle>
		<!--日出时间设置-->
		<AmOfDay>7</AmOfDay>
		<!--日落时间设置-->
		<PmOfDay>17</PmOfDay>
		<!--日间非隧道亮度登记-->
		<DayNotTunnel>3</DayNotTunnel>
		<!--其他段默认亮度等级-->
		<DefaultDayAndTunnel>2</DefaultDayAndTunnel>
		<!--画EB，SB三角形的外顶点时，前一个位置参数,重庆：20；其他线：13-->
		<PreESbPeak>13</PreESbPeak>
		<!--画EB，SB三角形的外顶点时，后一个位置参数，重庆：3；其他线：6-->
		<AfterESbPeak>6</AfterESbPeak>
	</Display>

	<!--目的地号配置数据,现在不使用该功能-->
	<DestNo>
		<!--目的地号显示配置：1为显示，0为不显示-->
		<Visible>1</Visible>
		<!--目的地号D字显示配置：1为显示，0为不显示,乌鲁木齐：1，其他：0-->
		<HeadCharVisible>0</HeadCharVisible>
		<!--目的地号显示范围配置-->
		<!--产品:99;贵阳:999;青岛:99;乌鲁木齐:99;越南:99;重庆环线:99;-->
		<MaxValue>999</MaxValue>
		<!--目的地号显示长度配置-->
		<!--产品：2;贵阳:3;青岛:2;乌鲁木齐：2;越南:2;重庆环线:2;-->
		<Len>3</Len>
	</DestNo>
	
	<!--车次号配置数据-->
	<TrainNo>
		<!--车次号显示配置：1为显示，0为不显示-->
		<Visible>1</Visible>
		<!--车次号T字显示配置：1为显示，0为不显示-->
		<!--产品:1;贵阳:0;青岛:1;乌鲁木齐:0;越南:1;重庆环线:0;-->
		<HeadCharVisible>0</HeadCharVisible>
		<!--车次号显示范围配置-->
		<!--产品:9999;贵阳:99999;青岛:9999;乌鲁木齐:99999;越南:9999;重庆环线:999999;-->
		<MaxValue>99999</MaxValue>
		<!--车次号显示长度配置-->
		<!--产品:4;贵阳:5;青岛:4;乌鲁木齐:5;越南:4;重庆环线:6;-->
		<Len>5</Len>
	</TrainNo>	
	
	<!--司机号配置数据-->
	<DriverNo>
		<!--司机号显示配置：1为显示，0为不显示-->
		<Visible>1</Visible>
		<!--司机号C字显示配置：1为显示，0为不显示-->
		<!--产品:1;贵阳:0;青岛:1;乌鲁木齐:1;越南:1;重庆环线:1;-->
		<HeadCharVisible>0</HeadCharVisible>
		<!--司机号显示最大值配置-->
		<!--产品:99999;贵阳：9999;青岛:99999;乌鲁木齐:99999;越南:99999;重庆环线:9999-->
		<MaxValue>9999</MaxValue>
		<!--司机号显示最小值配置-->
		<MinValue>1</MinValue>
		<!--司机号显示长度配置-->
		<!--产品:5;贵阳:4;青岛:5;乌鲁木齐:5;越南:5;重庆环线:4-->
		<Len>4</Len>
	</DriverNo>
</MMI>
